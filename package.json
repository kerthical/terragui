{
  "name": "@terragui/app",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build --turbo",
    "start": "next start",
    "lint:check": "tsgo && biome check",
    "lint:fix": "tsgo && biome check --write --unsafe",
    "db:migrate": "drizzle-kit push --dialect sqlite --casing snake_case --schema src/db/schema --url file:./terragui.db --force",
    "db:reset": "tsx ./src/scripts/reset.ts",
    "db:seed": "tsx ./src/scripts/seed.ts",
    "postinstall": "next typegen > /dev/null 2>&1 && tsx ./src/scripts/icon.ts && npm run lint:fix"
  },
  "dependencies": {
    "@ai-sdk/rsc": "^2.0.3",
    "@base-ui/react": "^1.0.0",
    "@hookform/resolvers": "^5.2.2",
    "@libsql/client": "^0.15.15",
    "@monaco-editor/react": "^4.7.0",
    "@paralleldrive/cuid2": "^3.0.4",
    "@tailwindcss/postcss": "^4.1.18",
    "@xyflow/react": "^12.10.0",
    "ai": "^6.0.3",
    "allotment": "^1.20.5",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "drizzle-orm": "^0.45.1",
    "elkjs": "^0.11.0",
    "embla-carousel-react": "^8.6.0",
    "input-otp": "^1.4.2",
    "lucide-react": "^0.562.0",
    "monaco-editor": "^0.55.1",
    "next": "^16.1.1",
    "next-themes": "^0.4.6",
    "postcss": "^8.5.6",
    "react": "^19.2.3",
    "react-day-picker": "^9.13.0",
    "react-dom": "^19.2.3",
    "react-hook-form": "^7.70.0",
    "react-resizable-panels": "^4.2.1",
    "recharts": "^3.6.0",
    "shadcn": "^3.6.2",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18",
    "tw-animate-css": "^1.4.0",
    "typescript-parsec": "^0.3.4",
    "vaul": "^1.1.2",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.10",
    "@commitlint/cli": "^20.4.2",
    "@commitlint/config-conventional": "^20.4.2",
    "@svgr/webpack": "^8.1.0",
    "@types/adm-zip": "^0.5.7",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@typescript/native-preview": "^7.0.0-dev.20260223.1",
    "adm-zip": "^0.5.16",
    "drizzle-kit": "^0.31.8",
    "lefthook": "^2.1.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
